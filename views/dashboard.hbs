<!--container start-->
<div class="container">

    <h1>Welcome Terry Crews{{agent}}</h1>

    <!-- Nav tabs -->
    <ul class="nav nav-tabs" role="tablist">
        <li class="nav-item">
            <a class="nav-link active" data-toggle="tab" href="#listings" role="tab">Listings</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" data-toggle="tab" href="#messages" role="tab">Messages</a>
        </li>
    </ul>

    <!--tab content start-->
    <div class="tab-content">

        <div class="tab-pane active" id="listings" role="tabpanel">
            <!--listing header start-->
            <div class="card">
                <div class="card-header">
                    <div class="row">
                        <h4 class="col-md-3">Address:</h4>
                        <h4 class="col-md-3">Listing Number:</h4>
                        <h4 class="col-md-3">Listing Options:</h4>
                    </div>
                </div>
            </div>
            <!--listing header end-->

            {{#each results}}
                <!--card listing start-->
                <div class="card">
                    <div class="card-header">
                        <div class="row">
                            <div class="col-md-3">
                                {{streetAddress}}
                            </div>
                            <div class="col-md-3">
                                {{id}}
                            </div>
                            <div class="col-md-6">
                                <form method="post" action="/dashboard">
                                    <button type="submit" class="btn btn-success" formaction="/dashboard/listing/{{id}}"
                                            name="listingdetailbutton" value="{{id}}">Go To Listing
                                    </button>
                                    <button type="submit" class="btn btn-primary" formaction="/edit" name="editButton"
                                            value="{{id}}">Edit Listing
                                    </button>
                                    <button type="submit" class="btn btn-danger" formaction="/dashboard/delete"
                                            name="deleteButton" value="{{id}}">Delete Listing
                                    </button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <!--card listing end-->
            {{/each}}
        </div>

        <!--messages tab pane start-->
        <div class="tab-pane" id="messages" role="tabpanel">
            <!--message header start-->
            <div class="card">
                <div class="card-header">
                    <div class="row">
                        <h4 class="col-md-3">Name:</h4>
                        <h4 class="col-md-3">Listing Number:</h4>
                        <h4 class="col-md-3">Date:</h4>
                    </div>
                </div>
            </div>
            <!--message header end-->

            <!--card messages start-->
            <div class="card">
                <div class="card-header">
                    <div class="row">
                        <div class="col-md-3">
                            Rick Sanchez{{firstName}} {{lastName}}
                        </div>

                        <div class="col-md-3">
                            12345 {{listingNumber}}
                        </div>
                        <div class="col-md-3">
                            11/9/17{{date}}
                        </div>
                        <div class="col-md-3">
                            <button type="button" class="btn btn-info pull-right" data-toggle="collapse"
                                    data-target="#messageOne">
                                View Message
                            </button>
                        </div>
                    </div>
                </div>
                <div class="card-block">
                    <div class="row">
                        <div id="messageOne" class="collapse col-md-12">
                            Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt
                            ut labore et dolore magna aliqua. Ut enim ad minim veniam,quis nostrud exercitation ullamco
                            laboris nisi ut aliquip ex ea commodo consequat.
                        </div>
                    </div>
                </div>
            </div>
            <!--card messages end-->


            <!--card messages start-->
            <div class="card">
                <div class="card-header">
                    <div class="row">
                        <div class="col-md-3">
                            Batman{{firstName}} {{lastName}}
                        </div>

                        <div class="col-md-3">
                            00000 {{listingNumber}}
                        </div>
                        <div class="col-md-3">
                            11/9/17{{date}}
                        </div>
                        <div class="col-md-3">
                            <button type="button" class="btn btn-info pull-right" data-toggle="collapse"
                                    data-target="#message">
                                View Message
                            </button>
                        </div>
                    </div>
                </div>
                <div class="card-block">
                    <div class="row">
                        <div id="message" class="collapse col-md-12">
                            Lorem ipsum dolor sit ametg, consectetur adipisicing elit, sed do eiusmod tempor incididunt
                            ut labore et dolore magna aliqua. Ut enim ad minim veniam,quis nostrud exercitation ullamco
                            laboris nisi ut aliquip ex ea commodo consequat.
                        </div>
                    </div>
                </div>
            </div>
            <!--card messages end-->

            <!--card messages start-->
            <div class="card">
                <div class="card-header">
                    <div class="row">
                        <div class="col-md-3">
                            Louis CK{{firstName}} {{lastName}}
                        </div>

                        <div class="col-md-3">
                            99999 {{listingNumber}}
                        </div>
                        <div class="col-md-3">
                            11/9/17{{date}}
                        </div>
                        <div class="col-md-3">
                            <button type="button" class="btn btn-info pull-right" data-toggle="collapse"
                                    data-target="#1{{messageNumber}}">
                                View Message
                            </button>
                        </div>
                    </div>
                </div>
                <div class="card-block">
                    <div class="row">
                        <div id="1{{messageNumber}}" class="collapse col-md-12">
                            Lorem ipsumf dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt
                            ut labore et dolore magna aliqua. Ut enim ad minim veniam,quis nostrud exercitation ullamco
                            laboris nisi ut aliquip ex ea commodo consequat.
                        </div>
                    </div>
                </div>
            </div>
            <!--card messages end-->

        </div>
        <!--messages tab pane end-->

    </div>
    <!--tab content end-->

</div>
<!--container end-->