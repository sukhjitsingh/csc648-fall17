<div class="container">
    <h1>Upload Images</h1>
    <hr>

    <!--breadcrumb start-->
    <nav class="breadcrumb">
        <span class="breadcrumb-item">Create Listing</span>
        <a class="breadcrumb-item" href="#">Upload Images</a>
    </nav>
    <!--breadcrumb end-->

    <h1>Photos of property</h1>
    <h6>Note: only up 10 photos allowed to upload.</h6>

    {{#if msg }}
        {{msg}}
    {{/if}}
    <form action="/listing" method="POST" enctype="multipart/form-data">
        <div class="file-field input-field">
            <div class="btn grey">
                <span>File</span>
                <input name="pics" type="file">
            </div>
            <button type="submit" class="btn btn-outline-secondary" formaction="/listing/upload">UploadPhoto</button>

        </div>
    </form>
    <br>

    <form method="POST" action="/listing">
        <button type="submit" class="btn btn-danger pull-left" formaction="/listing/cancel">Cancel</button>
        <button type="submit" class="btn btn-primary pull-right" formaction="/listing/finish">Create Listing</button>
    </form>
</div>


